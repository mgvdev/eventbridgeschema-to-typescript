{
  "name": "@mgvdev/eventbridgeschema-to-typescript",
  "version": "1.0.4",
  "description": "CLI tool to generate typescript interface from aws eventbridge schemas",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "bin": {
    "ebs2ts":"dist/index.js"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node ./dist/index.js",
    "lint": "eslint src/ --fix",
    "prettier": "prettier -c --write '**/*'",
    "test": "jest --config jest.config.json --no-cache",
    "test:watch": "jest --config jest.config.json --no-cache --watch",
    "prepare": "npm run build && npm run lint && npm run prettier && npm run test"
    
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prepare"
    }
  },
  
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soulmachine/ts-cli-starter.git"
  },
  "keywords": [
    "typescript",
    "CLI",
    "aws",
    "eventbridge",
    "schema",
    "interface"
  ],
  "engines": {
    "node": ">=10"
  },
  "author": "mgvdev",
  "license": "MIT",
  "devDependencies": {
    "@types/figlet": "^1.5.4",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.13",
    "@types/yargs": "^17.0.7",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.4.1",
    "aws-sdk-client-mock": "^2.0.1",
    "aws-sdk-client-mock-jest": "^2.0.1",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@aws-sdk/client-schemas": "^3.262.0",
    "chalk": "^5.0.0",
    "json-schema-to-typescript": "^11.0.3",
    "yargs": "^17.3.0"
  }
}
